{
    "bitwise": true,
    "browser": true,
    "camelcase": false,
    "curly": true,
    "esnext": true,
    "eqeqeq": true,
    "eqnull": true,
    "immed": true,
    "indent": 4,
    "latedef": true,
    "laxcomma": true,
    "maxlen": 180,
    "maxstatements": 20,
    "newcap": true,
    "node": true,
    "strict": true,
    "trailing": true,
    "quotmark": "single",
    "undef": true,
    "unused": true,
    "white": true,

    // Custom globals.
    "predef": [
        "describe",
        "define",
        "require",
        "it",
        "beforeEach",
        "before",
        "after",
        "afterEach"
    ]
}
